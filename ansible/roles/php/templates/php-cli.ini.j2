[PHP]

short_open_tag = Off

safe_mode = Off

max_execution_time = 600
max_input_time = 600

memory_limit = -1

error_reporting = E_ALL & ~E_DEPRECATED
display_errors = On

post_max_size = 50M
upload_max_filesize = 50M

allow_url_fopen = On
allow_url_include = Off

[Date]
date.timezone = Europe/Zurich

[intl]
intl.default_locale = de_CH.UTF-8